{
    "parser": "babel-eslint",
    "plugins": [
      "eslint-plugin-react"
    ],
    "rules": {
        "react/jsx-filename-extension": "off",
        "no-console": "off",
        "react/forbid-prop-types": "off",
        "no-shadow": "off",
        "no-case-declarations": "off",
        "no-underscore-dangle": 1
    }
}
